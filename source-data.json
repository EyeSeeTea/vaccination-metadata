            "$categoryOptions": { "kind": "fromAgeGroups" }
        },
        "dose": {
            "name": "_23.2 RVC Doses",
            "shortName": "Dose",
            "code": "RVC_DOSE",
            "dataDimensionType": "DISAGGREGATION",
            "dataDimension": true,
            "$categoryOptions": { "kind": "fromDoses" }
        },
        "team": {
            "name": "_23.4 RVC Teams",
            "shortName": "Team",
            "code": "RVC_TEAM",
            "dataDimensionType": "ATTRIBUTE",
            "dataDimension": true
        },
        "gender": {
            "name": "Gender",
            "shortName": "Gender",
            "code": "RVC_GENDER",
            "dataDimensionType": "DISAGGREGATION",
            "dataDimension": false,
            "$categoryOptions": { "kind": "values", "values": ["Female", "Male"] }
        },
        "severity": {
            "name": "_23.3 RVC Severity",
            "shortName": "Severity",
            "code": "RVC_SEVERITY",
            "dataDimensionType": "DISAGGREGATION",
            "dataDimension": true,
            "$categoryOptions": { "kind": "values", "values": ["Minor", "Severe"] }
        },
        "displacement-status": {
            "name": "_23.6 Displacement Status",
            "shortName": "Displacement Status",
            "code": "RVC_DISTATUS",
            "dataDimensionType": "DISAGGREGATION",
            "dataDimension": true,
            "$categoryOptions": { "kind": "values", "values": ["Host", "IDP", "Refugees"] }
        }
    },

    "categoryCombos": {
        "team": {
            "dataDimensionType": "ATTRIBUTE",
            "$categories": ["team"]
        },
        "antigen": {
            "$categories": ["antigen"]
        },
        "age-group": {
            "$categories": ["age-group"]
        },
        "antigen-age-group": {
            "$categories": ["antigen", "age-group"]
        },
        "antigen-dose-age-group": {
            "$categories": ["antigen", "dose", "age-group"]
        },
        "antigen-age-group-gender": {
            "$categories": ["antigen", "dose", "age-group", "gender"]
        },
        "antigen-age-group-displacement-status": {
            "$categories": ["antigen", "dose", "age-group", "displacement-status"]
        },
        "antigen-age-gender-displacement": {
            "$categories": ["antigen", "dose", "age-group", "gender", "displacement-status"]
        },
        "antigen-severity": {
            "$categories": ["antigen", "severity"]
        }
    },

    "indicatorTypes": {
        "ratio": {
            "name": "ratio",
            "factor": 1
        },
        "percentage": {
            "name": "percentage",
            "factor": 100
        }
    },

