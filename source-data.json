    "dataSets": {
        "demography": {
            "name": "Demographic Information",
            "code": "RVC_DEMOGRAPHIC_INFORMATION",
            "periodType": "Monthly",
            "dataElementDecoration": true,
            "renderAsTabs": true,
            "openFuturePeriods": 1,
            "timelyDays": 15,
            "$dataElements": ["total-population", "age-distribution"],
            "$organisationUnits": { "levels": [4, 5] },
            "$sections": {
                "Population": {
                    "sortOrder": 0,
                    "name": "Population Age Distribution (%)",
                    "showRowTotals": true,
                    "$dataElements": ["total-population", "age-distribution"]
