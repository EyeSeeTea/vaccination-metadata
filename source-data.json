    "validationRules": {
        "vaccine-doses": {
            "name": "Vaccine doses administered <= used (${antigen.name})",
            "code": "RVC_DOSES_${antigen.code}",
            "importance": "MEDIUM",
            "periodType": "Daily",
            "skipFormValidation": false,
            "leftSide": {
                "description": "Vaccine doses administered (${antigen.name})",
                "expression": "${getSumExpression('vaccine-doses-administered', antigen)}",
                "missingValueStrategy": "NEVER_SKIP",
                "slidingWindow": false
            },
            "operator": "less_than_or_equal_to",
            "rightSide": {
                "description": "Vaccine doses used (${antigen.name})",
                "expression": "${getSumExpression('vaccine-doses-used', antigen)}",
                "missingValueStrategy": "NEVER_SKIP",
                "slidingWindow": false
            }
        },
        "aefi": {
            "name": "No Adverse Event Following Immunization (${antigen.name})",
            "code": "RVC_AEFI_${antigen.code}",
            "instruction": "Please fill in the AEFI registration form (${antigen.name})",
            "importance": "MEDIUM",
            "periodType": "Daily",
            "skipFormValidation": false,
            "leftSide": {
                "description": "Adverse Event Following Immunization (${antigen.name})",
                "expression": "${getSumExpression('adverse-event', antigen)}",
                "missingValueStrategy": "NEVER_SKIP",
                "slidingWindow": false
            },
            "operator": "equal_to",
            "rightSide": {
                "description": "Zero",
                "expression": "0",
                "missingValueStrategy": "NEVER_SKIP",
                "slidingWindow": false
            }
        }
    },

    "indicators": {
        "ads-wastage-rate": {
            "$dataElementsRequired": ["ads-used", "vaccine-doses-administered"],
            "name": "ADS (Auto Disable Syringe) wastage rate (%)",
            "translations": [
                {
                    "property": "NAME",
                    "locale": "es",
                    "value": "Tasa de pérdida de Jeringas auto-desactivables (AD) (%)"
                },
                {
                    "property": "NAME",
                    "locale": "fr",
                    "value": "Taux de perte des serigues autobloquantes (SAB)"
