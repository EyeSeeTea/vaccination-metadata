    "vaccinationIndicators": {
        "global": {
            "ads-wastage-rate": {
                "name": "ADS (Auto Disable Syringe) wastage rate",
                "shortName": "ADS wastage rate",
                "code": "RVC_IN_AWR",
                "indicatorType": "percentage",
                "numeratorDescription": "(ADS used - Total Doses adm) / ADS used",
                "numerator": "#{${dataElements['ads-used'].id}} - (${dataElementGroups['vaccine-doses-administered'].dataElements.map(de => '#{' + de.id + '}').join(' + ')})",
                "denominatorDescription": "ADS used",
                "denominator": "#{${dataElements['ads-used'].id}}"
            },
            "safety-boxes-ratio": {
                "name": "Safety boxes ratio",
                "code": "RVC_IN_SBR",
                "indicatorType": "ratio",
                "numeratorDescription": "Syringes",
                "numerator": "#{${dataElements['syringes'].id}}",
                "denominatorDescription": "Safety Boxes",
                "denominator": "#{${dataElements['safety-boxes'].id}}"
            },
            "dilution-syringes-ratio": {
                "name": "Dilution syringes ratio",
                "code": "RVC_IN_DSR",
                "indicatorType": "percentage",
                "numeratorDescription": "Syringes for dilution",
                "numerator": "#{${dataElements['syringes-dilution'].id}}",
                "denominatorDescription": "Total doses used",
                "denominator": "${dataElementGroups['vaccine-doses-used'].dataElements.map(de => '#{' + de.id + '}').join(' + ')}"
            }
        },
        "byAntigen": {
            "vaccine-utilization-rate": {
                "name": "Vaccine utilization rate",
                "items": {
                    "key": "${antigen.key}-vaccine-utilization-rate",
                    "name": "${antigen.name} vaccine utilization rate",
                    "shortName": "${antigen.name} utilization rate",
                    "code": "RVC_IN_${antigen.code}_UR",
                    "indicatorType": "percentage",
                    "numeratorDescription": "Doses administered",
                    "numerator": "#{${dataElements[antigen.key + '-vaccine-doses-administered'].id}}",
                    "denominatorDescription": "Doses used",
                    "denominator": "#{${dataElements[antigen.key + '-vaccine-doses-used'].id}}"
                }
            },
            "vaccine-campaign-coverage": {
                "name": "Vaccine campaign coverage",
                "items": {
                    "key": "${antigen.key}-vaccine-campaign-coverage",
                    "name": "${antigen.name} vaccine campaign coverage",
                    "shortName": "${antigen.name} campaign coverage",
                    "code": "RVC_IN_${antigen.code}_CC",
                    "indicatorType": "percentage",
                    "numeratorDescription": "Doses administered",
                    "numerator": "#{${dataElements[antigen.key + '-vaccine-doses-administered'].id}}",
                    "denominatorDescription": "Total population",
                    "denominator": "#{${dataElements['total-population'].id}}"
                }
            }
        }
    },

    "vaccinationData": {
        "global": {
            "total-population": {
                "name": "Total population",
                "code": "RVC_AD_TP",
                "valueType": "INTEGER_ZERO_OR_POSITIVE"
            },
            "ads-used": {
                "name": "ADS (Auto Disable Syringe) used",
                "code": "RVC_AD_ADS1",
                "valueType": "INTEGER_ZERO_OR_POSITIVE"
            },
            "ads-dilution": {
