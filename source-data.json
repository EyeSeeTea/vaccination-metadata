    "dataElements": {
        "total-population": {
            "name": "Total population (catchment/target)",
            "code": "DEM1",
            "valueType": "NUMBER"
        },
        "total-population-by-age": {
            "name": "Total population by age (catchment/target)",
            "code": "DEM4",
            "valueType": "NUMBER"
        },
        "age-distribution": {
            "name": "Age Distribution",
            "valueType": "NUMBER",
            "code": "DEM3"
        },
        "comments-demographic": {
            "name": "Comments DS_DEM",
            "valueType": "LONG_TEXT",
            "code": "DE_DS_DEM"
        },
        "comments-population": {
            "name": "Comments DS_POP",
            "valueType": "LONG_TEXT",
            "code": "DE_DS_POP"
        },
        "birth-rate": {
            "name": "Crude Birth Rate (%)",
            "valueType": "NUMBER",
            "code": "DEM2"
        },

        "vaccine-doses-administered": {
            "$byAntigen": true,
            "name": "Vaccine doses administered",
            "shortName": "Doses adm",
            "code": "RVC_AD_DA",
            "valueType": "NUMBER",
            "$categoryCombo": "age-group-${antigen.key}"
        },
        "vaccine-doses-used": {
            "$byAntigen": true,
            "name": "Vaccine doses used",
            "shortName": "Doses used",
            "code": "RVC_AD_DU",
            "valueType": "NUMBER",
            "$categoryCombo": "age-group-${antigen.key}"
        },
        "ads-used": {
            "$byAntigen": true,
            "name": "ADS (Auto Disable Syringe) used",
            "shortName": "ADS used",
            "code": "RVC_AD_ADSU",
            "valueType": "NUMBER"
        },
        "ads-dilution": {
            "$byAntigen": true,
            "name": "ADS (Auto Disable Syringe) for dilution",
            "shortName": "ADS dilution",
            "code": "RVC_AD_ADSD",
            "valueType": "NUMBER"
        },
        "safety-boxes": {
            "$byAntigen": true,
            "name": "Safety boxes",
            "code": "RVC_AD_SB",
            "valueType": "NUMBER"
        },
        "syringes": {
            "$byAntigen": true,
            "name": "Syringes",
            "code": "RVC_AD_SYR",
            "valueType": "NUMBER"
        },
        "syringes-dilution": {
            "$byAntigen": true,
            "name": "Syringes for dilution",
            "code": "RVC_AD_SYD",
            "valueType": "NUMBER"
        },
        "needles": {
            "$byAntigen": true,
            "name": "Needles",
            "key": "needles",
            "code": "RVC_AD_NDL",
            "valueType": "NUMBER"
        },
        "aeb": {
            "$byAntigen": true,
            "name": "Accidental Exposure to Blood (AEB)",
            "shortName": "AEB",
            "code": "RVC_AD_AEB",
            "valueType": "NUMBER"
        },
        "adverse-event": {
            "$byAntigen": true,
            "name": "Adverse Event Following Immunization",
            "shortName": "AEFI",
            "code": "RVC_AD_AEFI",
            "valueType": "NUMBER",
            "$disaggregation": "severity"
        }
    },

    "indicators": {
        "ads-wastage-rate": {
            "$byAntigen": true,
            "name": "ADS (Auto Disable Syringe) wastage rate",
            "shortName": "ADS wastage rate",
            "code": "RVC_IN_AWR",
            "$indicatorType": "percentage",
            "numeratorDescription": "ADS used - Total Doses adm",
            "numerator": "#{${dataElements['ads-used-' + antigen.key].id}} - #{${dataElements['vaccine-doses-administered-' + antigen.key].id}}",
            "denominatorDescription": "ADS used",
            "denominator": "#{${dataElements['ads-used-' + antigen.key].id}}"
        },
        "safety-boxes-ratio": {
            "$byAntigen": true,
            "name": "Safety boxes ratio",
            "code": "RVC_IN_SBR",
            "$indicatorType": "ratio",
            "numeratorDescription": "Syringes",
            "numerator": "#{${dataElements['syringes-' + antigen.key].id}}",
            "denominatorDescription": "Safety Boxes",
            "denominator": "#{${dataElements['safety-boxes-' + antigen.key].id}}"
        },
        "dilution-syringes-ratio": {
            "$byAntigen": true,
            "name": "Dilution syringes ratio",
            "code": "RVC_IN_DSR",
            "$indicatorType": "percentage",
            "numeratorDescription": "Syringes for dilution",
            "numerator": "#{${dataElements['syringes-dilution-' + antigen.key].id}}",
            "denominatorDescription": "Total doses used",
            "denominator": "#{${dataElements['vaccine-doses-used-' + antigen.key].id}}"
        },
        "vaccine-utilization-rate": {
            "$byAntigen": true,
            "name": "Vaccine utilization rate",
            "shortName": "Utilization rate",
            "code": "RVC_IN_UR",
            "$indicatorType": "percentage",
            "numeratorDescription": "Doses administered",
            "numerator": "#{${dataElements['vaccine-doses-administered-' + antigen.key].id}}",
            "denominatorDescription": "Doses used",
            "denominator": "#{${dataElements['vaccine-doses-used-' + antigen.key].id}}"
        },
        "vaccine-campaign-coverage": {
            "$byAntigen": true,
            "name": "Vaccine campaign coverage",
            "shortName": "Campaign coverage",
            "code": "RVC_IN_CC",
            "$indicatorType": "percentage",
            "numeratorDescription": "Doses administered",
            "numerator": "#{${dataElements['vaccine-doses-administered-' + antigen.key].id}}",
            "denominatorDescription": "Total population",
            "denominator": "#{${dataElements['total-population'].id}}"
        }
    },

