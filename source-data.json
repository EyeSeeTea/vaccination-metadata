    "dataElements": {
        "total-population": {
            "name": "Total population",
            "code": "RVC_TOTAL_POPULATION",
            "aggregationType": "LAST",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "aggregationLevels": [1, 2, 3, 4, 5, 6]
        },
        "age-distribution": {
            "name": "Age distribution (%)",
            "valueType": "NUMBER",
            "code": "RVC_AGE_DISTRIBUTION",
            "aggregationType": "LAST",
            "$categoryCombo": "age-group",
            "aggregationLevels": [1, 2, 3, 4, 5, 6]
        },
        "population-by-age": {
            "name": "Population by age",
            "code": "RVC_POPULATION_BY_AGE",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "aggregationType": "LAST",
            "$categoryCombo": "antigen-age-group",
            "aggregationLevels": []
        },

        "vaccine-doses-administered": {
            "name": "Vaccine doses administered",
            "code": "RVC_DOSES_ADMINISTERED",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 1,
            "$categoryCombo": "antigen-age-gender-displacement",
            "$categories": [
                { "key": "antigen", "optional": false },
                { "key": "dose", "optional": false },
                { "key": "age-group", "optional": false },
                { "key": "gender", "optional": true },
                { "key": "displacement-status", "optional": true }
            ]
        },
        "vaccine-doses-used": {
            "name": "Vaccine doses used",
            "code": "RVC_DOSES_USED",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 2,
            "$categoryCombo": "antigen",
            "$categories": [{ "key": "antigen", "optional": false }]
        },

        "ads-used": {
            "name": "ADS (Auto Disable Syringe) used",
            "code": "RVC_ADS_USED",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 3,
            "$categoryCombo": null
        },
        "syringes": {
            "name": "Syringes for dilution",
            "code": "RVC_SYRINGES",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 5,
            "$categoryCombo": "antigen",
            "$categories": [{ "key": "antigen", "optional": false }]
        },
        "needles": {
            "name": "Needles for dilution",
            "key": "needles",
            "code": "RVC_NEEDLES",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 4,
            "$categoryCombo": "antigen",
            "$categories": [{ "key": "antigen", "optional": false }]
        },
        "safety-boxes": {
            "name": "Safety boxes",
            "code": "RVC_SAFETY_BOXES",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 6,
            "$categoryCombo": null
        },
        "aeb": {
            "name": "Accidental Exposure to Blood (AEB)",
            "code": "RVC_AEB",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 7,
            "$categoryCombo": "antigen",
            "$categories": [{ "key": "antigen", "optional": false }]
        },
        "adverse-event": {
            "name": "Adverse Event Following Immunization",
            "code": "RVC_AEFI",
            "valueType": "INTEGER_ZERO_OR_POSITIVE",
            "$order": 8,
            "$categoryCombo": ["antigen-severity"],
            "$categories": [
                { "key": "antigen", "optional": false },
                { "key": "severity", "optional": false }
            ]
        }
    },

    "indicators": {
        "ads-wastage-rate": {
            "$dataElementsRequired": ["ads-used", "vaccine-doses-administered"],
            "name": "ADS (Auto Disable Syringe) wastage rate",
            "code": "RVC_ADS_WASTAGE",
            "$indicatorType": "percentage",
            "numeratorDescription": "ADS used - Total Doses adm",
            "numerator": "#{${dataElements['ads-used']}} - #{${dataElements['vaccine-doses-administered']}}",
            "denominatorDescription": "ADS used",
            "denominator": "#{${dataElements['ads-used']}}"
        },
        "safety-boxes-ratio": {
            "$dataElementsRequired": ["syringes", "safety-boxes"],
            "name": "Safety boxes ratio",
            "code": "RVC_SAFETY_BOXES",
            "$indicatorType": "ratio",
            "numeratorDescription": "Syringes",
            "numerator": "#{${dataElements['syringes']}}",
            "denominatorDescription": "Safety Boxes",
            "denominator": "#{${dataElements['safety-boxes']}}"
        },
        "dilution-syringes-ratio": {
            "$dataElementsRequired": ["syringes", "vaccine-doses-used"],
            "name": "Dilution syringes ratio",
            "code": "RVC_DILUTION_SYRINGES_RATIO",
            "$indicatorType": "percentage",
            "numeratorDescription": "Syringes for dilution",
            "numerator": "#{${dataElements['syringes']}}",
            "denominatorDescription": "Total doses used",
            "denominator": "#{${dataElements['vaccine-doses-used']}}"
        },
        "vaccine-utilization-rate": {
            "$dataElementsRequired": ["vaccine-doses-administered", "vaccine-doses-used"],
            "name": "Vaccine utilization rate",
            "code": "RVC_VACCINE_UTILIZATION",
            "$indicatorType": "percentage",
            "numeratorDescription": "Doses administered",
            "numerator": "#{${dataElements['vaccine-doses-administered']}}",
            "denominatorDescription": "Doses used",
            "denominator": "#{${dataElements['vaccine-doses-used']}}"
        },
        "vaccine-campaign-coverage": {
            "$dataElementsRequired": ["vaccine-doses-administered", "population-by-age"],
            "name": "Vaccine campaign coverage",
            "code": "RVC_CAMPAIGN_COVERAGE",
            "$indicatorType": "percentage",
            "numeratorDescription": "Doses administered",
            "numerator": "#{${dataElements['vaccine-doses-administered']}}",
            "denominatorDescription": "Population by age",
            "denominator": "#{${dataElements['population-by-age']}}"
        }
    },

    "attributes": {
        "create-by-app": {
            "name": "Created by Vaccination App",
            "code": "RVC_CREATED_BY_VACCINATION_APP",
            "valueType": "BOOLEAN",
            "dataSetAttribute": true
        },
        "dashboard-id": {
            "name": "Dashboard ID",
            "code": "RVC_DASHBOARD_ID",
            "valueType": "TEXT",
            "dataSetAttribute": true
        }
    },

    "categories": {
        "antigen": {
            "name": "_23.1 RVC Antigen",
