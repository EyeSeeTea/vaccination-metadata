    "dataSets": {
        "demograhic-yearly": {
            "name": "Demographic Information - Yearly",
            "shortName": "Demographic Information",
            "code": "DS_DEM",
            "periodType": "Yearly",
            "dataElementDecoration": true,
            "renderAsTabs": true,
            "openFuturePeriods": 1,
            "timelyDays": 15,
            "$dataElements": ["age-distribution", "comments-demographic", "birth-rate"],
            "$organisationUnitsLevels": {"names": ["OCBA"], "levels": [4, 5]},
            "$sections": {
                "birth-rate": {
                    "name": "Crude Birth Rate (%)",
                    "showRowTotals": true,
                    "$dataElements": ["birth-rate"]
                },
                "age-distribution": {
                    "name": "Population Age Distribution (%)",
                    "showRowTotals": true,
                    "$dataElements": ["age-distribution"]
                },
                "comments": {
                    "name": "comments",
                    "$dataElements": ["comments-demographic"]
                }
            }
        },
        "population-quarterly": {
            "name": "Population - Quarterly",
            "shortName": "Population - Quarterly",
            "code": "DS_POP_Q",
            "periodType": "Quarterly",
            "dataElementDecoration": true,
            "renderAsTabs": true,
            "openFuturePeriods": 4,
            "timelyDays": 15,
            "$dataElements": [
                "total-population",
                "total-population-by-age",
                "comments-population"
            ],
            "$organisationUnitsLevels": {"names": ["OCBA"], "levels": [4, 5]},
            "$sections": {
                "population": {
                    "name": "Population",
                    "showRowTotals": true,
                    "$dataElements": [
                        "total-population",
                        "total-population-by-age"
                    ]
                },
                "comments": {
                    "name": "comments",
                    "$dataElements": [
                        "comments-population"
                    ]
                }
