            },
            "adverse-event": {
                "name": "Adverse Event Following Immunization",
                "code": "RVC_AD_AEFI",
                "valueType": "INTEGER_ZERO_OR_POSITIVE",
                "disaggregation": "severity"
            }
        },
        "byAntigen": {
            "vaccine-doses-administered": {
                "name": "Vaccine doses administered",
                "items": {
                    "key": "${antigen.key}-vaccine-doses-administered",
                    "name": "${antigen.name} vaccine doses administered",
                    "shortName": "${antigen.name} doses adm",
                    "code": "RVC_AD_${antigen.code}_DA",
                    "valueType": "INTEGER_ZERO_OR_POSITIVE"
                }
            },
            "vaccine-doses-used": {
                "name": "Vaccine doses used",
                "items": {
                    "key": "${antigen.key}-vaccine-doses-used",
                    "name": "${antigen.name} vaccine doses used",
                    "shortName": "${antigen.name} doses used",
                    "code": "RVC_AD_${antigen.code}_DU",
                    "valueType": "INTEGER_ZERO_OR_POSITIVE"
                }
            }
        }
    },

    "categories": {
